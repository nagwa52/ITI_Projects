            <Font name="Forte" size="65.0" />
         </font>
      </Text>
      <ImageView fitHeight="196.0" fitWidth="354.0" layoutX="756.0" layoutY="12.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@the_best.png" />
         </image>
      </ImageView>
      <TableView fx:id="playersListTable" layoutX="44.0" layoutY="87.0" onMouseClicked="#MouseClicked" prefHeight="511.0" prefWidth="698.0">
