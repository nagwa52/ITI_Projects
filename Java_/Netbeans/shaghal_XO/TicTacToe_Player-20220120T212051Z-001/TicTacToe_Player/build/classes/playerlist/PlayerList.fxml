<AnchorPane id="AnchorPane" prefHeight="589.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="playerlist.PlayerListController">
   <children>
